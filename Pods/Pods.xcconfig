GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/AprilSDK" "${PODS_ROOT}/Headers/FMDB" "${PODS_ROOT}/Headers/MBProgressHUD" "${PODS_ROOT}/Headers/ReactiveCocoa" "${PODS_ROOT}/Headers/ReactiveCocoa/ReactiveCocoa"
LIBRARY_SEARCH_PATHS = $(PODS_ROOT)/AprilSDK "$(PODS_ROOT)/AprilSDK/AprilSDK"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/AprilSDK" -isystem "${PODS_ROOT}/Headers/FMDB" -isystem "${PODS_ROOT}/Headers/MBProgressHUD" -isystem "${PODS_ROOT}/Headers/ReactiveCocoa" -isystem "${PODS_ROOT}/Headers/ReactiveCocoa/ReactiveCocoa"
OTHER_LDFLAGS = -ObjC -lAprilBeaconSDK -lsqlite3 -framework CoreGraphics
PODS_ROOT = ${SRCROOT}/Pods